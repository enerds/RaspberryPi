body			{ background: #FFFFFF; color: #000000; font: 100% Verdana, sans-serif }
a:link			{ color: #0033FF; text-decoration: none }
a:visited		{ color: #0022CC; text-decoration: none }
a:focus			{ color: #2244FF; text-decoration: none }
a:hover			{ color: #4466FF; text-decoration: none }
a:active		{ color: #6688FF; text-decoration: none }
div			{ display: block; margin: .5%; background: #EEEEEE; color: #000000 }
form			{ margin: 0; padding: 0 }
h2			{ font-size: 120%; font-weight: bold }
img			{ border: none }
table			{ border: solid 1px #001144; background: #DDFFFF; font: 100% Verdana, sans-serif; width: 100%; margin: 0 }
tr			{ border-style: solid; background: #EEFFFF; border-width: 1px 0 }
tr.alt			{ border-style: solid; background: #CCF4F4; border-width: 1px 0 }
tr.head			{ border-style: solid; background: #B0DDF0; border-width: 1px 0 }
td a			{ display: block; width: 100%; height: 100% }
table.anotfull td a	{ display: inline; width: auto; height: auto }
td.anotfull a		{ display: inline; width: auto; height: auto }
#topbar			{ width: 99%; clear: both }
.button			{ background: #AAAAAA; color: #000000; border-style: solid; border-width: 2px; border-color: #DDDDDD #888888 #888888 #DDDDDD }
.textbox		{ background: #FFFFFF; color: #000000; border-style: solid; border-width: 2px; border-color: #888888 #DDDDDD #DDDDDD #888888 }
.nostyle		{ border: none; background: none; font: 100% Verdana, sans-serif }
.hilight		{ border: none; background: #B0DDF0; color: #000000; font: 100% Verdana, sans-serif }
.sliderdark		{ margin: 0; padding: 0; width: 4px; height: 16px; background-color: #FF0000; border: none; float: left }
.sliderlite		{ margin: 0; padding: 0; width: 4px; height: 16px; background-color: #AAAAAA; border: none; float: left }
a:link .sliderdark	{ border-color: #AAAAAA; background: #888888; text-decoration: none; margin: 0; padding: 0 }
a:visited .sliderdark	{ border-color: #AAAAAA; background: #888888; text-decoration: none; margin: 0; padding: 0 }
a:focus .sliderdark	{ border-color: #CCCCCC; background: #999999; text-decoration: none; margin: 0; padding: 0 }
a:hover .sliderdark	{ border-color: #EEEEEE; background: #AAAAAA; text-decoration: none; margin: 0; padding: 0 }
a:active .sliderdark	{ border-color: #FFFFFF; background: #CCCCCC; text-decoration: none; margin: 0; padding: 0 }
a:link .sliderlite	{ border-color: #000FF0; background: #000BB0; text-decoration: none; margin: 0; padding: 0 }
a:visited .sliderlite	{ border-color: #000FF0; background: #000BB0; text-decoration: none; margin: 0; padding: 0 }
a:focus .sliderlite	{ border-color: #333FF3; background: #000DD0; text-decoration: none; margin: 0; padding: 0 }
a:hover .sliderlite	{ border-color: #666FF6; background: #000FF0; text-decoration: none; margin: 0; padding: 0 }
a:active .sliderlite	{ border-color: #999FF9; background: #333FF3; text-decoration: none; margin: 0; padding: 0 }
.hilight a:link		{ color: #000880; text-decoration: none }
.hilight a:visited	{ color: #000660; text-decoration: none }
.hilight a:focus	{ color: #000AA0; text-decoration: none }
.hilight a:hover	{ color: #000BB0; text-decoration: none }
.hilight a:active	{ color: #000CC0; text-decoration: none }
