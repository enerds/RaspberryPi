body			{ background: #FFFFFF; color: #000000; font: 100% Verdana, sans-serif }
div			{ display: block; margin: .5%; background: none; color: #000000 }
form			{ margin: 0; padding: 0 }
img			{ border: none }
a:link			{ color: #0000FF; text-decoration: none }
a:visited		{ color: #0000CC; text-decoration: none }
a:focus			{ color: #2222FF; text-decoration: none }
a:hover			{ color: #4444FF; text-decoration: none }
a:active		{ color: #6666FF; text-decoration: none }
a:link .sliderdark	{ border-color: #AAAAAA; background: #888888; text-decoration: none; margin: 0; padding: 0 }
a:visited .sliderdark	{ border-color: #AAAAAA; background: #888888; text-decoration: none; margin: 0; padding: 0 }
a:focus .sliderdark	{ border-color: #CCCCCC; background: #999999; text-decoration: none; margin: 0; padding: 0 }
a:hover .sliderdark	{ border-color: #EEEEEE; background: #AAAAAA; text-decoration: none; margin: 0; padding: 0 }
a:active .sliderdark	{ border-color: #FFFFFF; background: #CCCCCC; text-decoration: none; margin: 0; padding: 0 }
a:link .sliderlite	{ border-color: #000000; background: #000000; text-decoration: none; margin: 0; padding: 0 }
a:visited .sliderlite	{ border-color: #000000; background: #000000; text-decoration: none; margin: 0; padding: 0 }
a:focus .sliderlite	{ border-color: #333333; background: #000000; text-decoration: none; margin: 0; padding: 0 }
a:hover .sliderlite	{ border-color: #666666; background: #000000; text-decoration: none; margin: 0; padding: 0 }
a:active .sliderlite	{ border-color: #999999; background: #333333; text-decoration: none; margin: 0; padding: 0 }
.hilight a:link		{ color: #000088; text-decoration: none }
.hilight a:visited	{ color: #000066; text-decoration: none }
.hilight a:focus	{ color: #0000AA; text-decoration: none }
.hilight a:hover	{ color: #0000BB; text-decoration: none }
.hilight a:active	{ color: #0000CC; text-decoration: none }
h2			{ display: block; width: 100%; font-size: 105%; font-weight: bold; background: #bbbbff; margin-bottom: 0; padding-bottom: 2px; border: solid 1px; border-color: #bbbbff }
table			{ border: solid 2px #bbbbff; background: #ffffff; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
tr			{ border: none; background: #eeeeff; padding: 0; margin: 0 }
tr.alt td		{ background: #ddddff }
tr.head td		{ background: #aaaaff }
td.nobg			{ background: #bbbbff; border-color: #bbbbff }
td			{ border: none; padding: 1px; margin: 0 }
#playlist h2		{ background: #ffaaaa; border-color: #ffaaaa }
#playlist table		{ border: solid 2px #ffaaaa; background: #ffffff; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
#playlist tr		{ background: #ffdddd }
#playlist tr.alt td	{ background: #ffeeee }
#playlist tr.head td	{ background: #ff8888 }
#playlist td.nobg	{ background: #ffaaaa; border-color: #ffaaaa }
#playlists h2		{ background: #ffaaaa; border-color: #ffaaaa }
#playlists table	{ border: solid 2px #ffaaaa; background: #ffffff; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
#playlists tr		{ background: #ffdddd }
#playlists tr.alt td	{ background: #ffeeee }
#playlists tr.head td	{ background: #ff8888 }
#playlists td.nobg	{ background: #ffaaaa; border-color: #ffaaaa }
#files h2		{ background: #aaffaa; border-color: #aaffaa }
#files table		{ border: solid 2px #aaffaa; background: #ffffff; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
#files tr		{ background: #ccffcc }
#files tr.alt td	{ background: #eeffee }
#files tr.head td	{ background: #88ff88 }
#files td.nobg		{ background: #aaffaa; border-color: #aaffaa }
#directories h2		{ background: #bbbbff; border-color: #bbbbff }
#directories table	{ border: solid 2px #bbbbff; background: #ffffff; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
#directories tr		{ background: #eeeeff }
#directories tr.alt td	{ background: #ddddff }
#directories tr.head td	{ background: #aaaaff }
#directories td.nobg	{ background: #bbbbff; border-color: #bbbbff }
#status h2		{ background: #cccccc; border-color: #cccccc }
#status table		{ border: solid 2px #eeeeee; background: #eeeeee; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
#status tr		{ background: #eeeeee }
#status td.nobg		{ background: #eeeeee; border-color: #eeeeee }
#control h2		{ background: #cccccc; border-color: #cccccc }
#control table		{ border: solid 2px #eeeeee; background: #eeeeee; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
#control tr		{ background: #eeeeee }
#control td.nobg	{ background: #eeeeee; border-color: #eeeeee }
td a			{ display: block; width: 100%; height: 100% }
table.anotfull		{ border: none; background: #eeeeff; font: 100% Verdana, sans-serif; width: 100%; margin: 0; padding: 0 }
table.anotfull td	{ border: none; padding: 0; margin: 0 }
table.anotfull td a	{ display: inline; width: auto; height: auto }
td.anotfull a		{ display: inline; width: auto; height: auto }
#topbar			{ width: 99%; clear: both }
.button			{ background: #AAAAAA; color: #000000; border-style: solid; border-width: 2px; border-color: #DDDDDD #888888 #888888 #DDDDDD }
.textbox		{ background: #FFFFFF; color: #000000; border-style: solid; border-width: 2px; border-color: #888888 #DDDDDD #DDDDDD #888888 }
.nostyle		{ border: none; background: none; font: 100% Verdana, sans-serif; padding: 0; margin: 0 }
.nostyle tr		{ padding: 0; margin: 0 }
.nostyle td		{ padding-top: 0; padding-bottom: 0; margin: 0 }
.hilight		{ border: none; background: #bbbbff; color: #000000; font: 100% Verdana, sans-serif }
#playlist .hilight	{ border: none; background: #88ff88; color: #000000; font: 100% Verdana, sans-serif }
.sliderdark		{ margin: 0; padding: 0; width: 4px; height: 16px; background-color: #000000; border: none; float: left }
.sliderlite		{ margin: 0; padding: 0; width: 4px; height: 16px; background-color: #aaaaaa; border: none; float: left }
